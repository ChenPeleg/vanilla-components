{
  "$schema": "http://json.schemastore.org/web-types",
  "name": "vanilla-elements",
  "version": "1.0.0",
  "contributions": {
    "html": {
      "elements": [
        {
          "name": "app-button",
          "description": "A button component with optional class and disabled attributes.",
          "source": { "module": "./src/components/atoms/app-button.ts",
            "symbol": "app-button"
          },
          "attributes": [
            { "name": "class", "description": "CSS classes for the button." },
            { "name": "disabled", "description": "Disables the button if set to true." }
          ]
        },
        {
          "name": "card-component",
          "description": "A card UI component with header and height attributes.",
          "source": { "module": "./src/components/atoms/card-component.ts" },
          "attributes": [
            { "name": "header", "description": "Header text for the card." },
            { "name": "height", "description": "Height of the card." }
          ]
        },
        {
          "name": "text-input",
          "description": "A text input component.",
          "source": { "module": "./src/components/atoms/text-input.ts" },
          "attributes": [
            { "name": "defaultValue", "description": "Default value for the input." }
          ],
          "properties": [
            { "name": "value", "description": "Current value of the input." }
          ]
        },
        {
          "name": "toggle-button",
          "description": "A toggle button component.",
          "source": { "module": "./src/components/atoms/toggle-button.ts" },
          "attributes": [
            { "name": "defaultValue", "description": "Initial toggle state." },
            { "name": "isActive", "description": "Current toggle state (true/false)." }
          ]
        },
        {
          "name": "logo-component",
          "description": "A logo display component.",
          "source": { "module": "./src/components/molecules/logo-component.ts" },
          "attributes": [
            { "name": "width", "description": "Width of the logo in pixels." }
          ]
        },
        {
          "name": "hero-section",
          "description": "A hero section with static content.",
          "source": { "module": "./src/components/molecules/hero-section.ts" }
        },
        {
          "name": "nav-bar",
          "description": "A navigation bar component.",
          "source": { "module": "./src/components/organism/nav-bar.ts" },
          "attributes": [
            { "name": "header", "description": "Header text for the navigation bar." },
            { "name": "text", "description": "Additional text for the navigation bar." }
          ]
        },
        {
          "name": "state-management-panel",
          "description": "Panel for state management demo.",
          "source": { "module": "./src/components/organism/state-management-panel.ts" }
        },
        {
          "name": "main-content",
          "description": "Main content area.",
          "source": { "module": "./src/components/organism/main-content.ts" }
        },
        {
          "name": "card-panel",
          "description": "Panel containing a card component.",
          "source": { "module": "./src/components/organism/card-panel.ts" }
        },
        {
          "name": "router-outlet",
          "description": "Router outlet for rendering routes.",
          "source": { "module": "./src/_core/router/router-outlet.ts" }
        },
        {
          "name": "home-page",
          "description": "Home page component.",
          "source": { "module": "./src/pages/home-page.ts" }
        },
        {
          "name": "about-page",
          "description": "About page component.",
          "source": { "module": "./src/pages/about-page.ts" }
        },
        {
          "name": "main-layout",
          "description": "Main layout wrapper.",
          "source": { "module": "./src/pages/main.layout.ts" }
        },
        {
          "name": "app-root",
          "description": "Root application element.",
          "source": { "module": "./src/root.ts" }
        }
      ]
    }
  }
}
